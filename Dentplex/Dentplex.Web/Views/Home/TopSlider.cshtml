@model IEnumerable<Dentplex.Data.Model.SliderItem>
@if (Model.Any())
{
    <div class="wrapp-slider">
        <div class="slider">
            <ul class="slider-main">
                @foreach (var item in Model)
                {

                    <li>
                        <a href="@item.SlideItemLink"><img src="~/Images/SliderItemImages/@item.SlideItemImage" alt=""></a>
                    </li>

                }
            </ul>
        </div>
    </div>
}

